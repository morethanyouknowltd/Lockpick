  "name": "lockpick-app",
  "description": "Productivity enhancer for music producers",
  "main": "./dist/main/main.js",
  "scripts": {
    "preinstall": "npx only-allow yarn",
    "configure": "node-gyp configure",
    "watch": "tsc -w --p ./src/main/tsconfig.json",
    "build": "yarn build:renderer && yarn build:main && yarn buildc && yarn build:controller",
    "build:renderer": "vite build",
    "build:main": "npm run buildc && tsc --p",
    "build:arm": "FOR_REAL=true yarn electron-builder build --arm64",
    "buildc:dev": "node-gyp -j 16 build --debug",
    "buildc": "node-gyp -j 16 build",
    "rebuildc:dev": "node-gyp -j 16 rebuild --debug",
    "rebuildc": "node-gyp -j 16 rebuild",
    "cleanc": "node-gyp clean",
    "build:controller": "tsc --p tsconfig.controller-script.json",
    "watch:controller": "tsc -w --p tsconfig.controller-script.json",
    "postinstall": "./scripts/update-cpp-properties.js",
    "generate-api": "./scripts/generate-api.sh"
  },
