    "source-map-loader": "^0.2.3",
    "titleizejs": "^1.0.2",
    "ts-loader": "^6.2.0",
    "tslint": "^5.8.0",
    "typescript": "^3.6.4",
    "webpack": "^4.44.1",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.2",
